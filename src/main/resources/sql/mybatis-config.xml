		<typeAlias alias="chat" type="com.crud.ffin.service.domain.Chat" />
		<typeAlias alias="comment" 	type="com.crud.ffin.service.domain.Comment" />
		<typeAlias alias="coupon" 	type="com.crud.ffin.service.domain.Coupon" />
		<typeAlias alias="heart" 	type="com.crud.ffin.service.domain.Heart" />
		<typeAlias alias="inquiry" 		type="com.crud.ffin.service.domain.Inquiry" />
		<typeAlias alias="menu" 	type="com.crud.ffin.service.domain.Menu" />
		<typeAlias alias="msg" 	type="com.crud.ffin.service.domain.Msg" />
