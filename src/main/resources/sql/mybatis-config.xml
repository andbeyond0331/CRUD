		<typeAlias alias="truck" 		type="com.crud.ffin.service.domain.Truck" />
		<typeAlias alias="user" 	type="com.crud.ffin.service.domain.User" />
	</typeAliases>



	<mappers>
		<mapper resource="sql/CmntMapper.xml"/>
		<mapper resource="sql/MenuMapper.xml"/>
		<mapper resource="sql/PurchaseMapper.xml"/>
		<mapper resource="sql/ResMapper.xml"/>
		<mapper resource="sql/ReviewMapper.xml"/>
		<mapper resource="sql/TruckMapper.xml"/>
		<mapper resource="sql/UserMapper.xml"/>
